-MD
-O2
-mtune=i586
-march=i386
-Wall

-Wcast-qual
-Werror
-Wno-error=stringop-truncation
-Wno-error=nonnull-compare
-Wno-error=format-overflow
-Wno-error=stringop-overflow
-Wno-error=misleading-indentation
-Wno-error=unused-const-variable


-Wpointer-arith
-Wshadow

-Wwrite-strings
-Wundef
-Wcast-align
-Wsign-compare
-nostdinc
-iquote .
